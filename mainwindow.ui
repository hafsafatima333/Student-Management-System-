   <string notr="true">/* 1. Main Background */
QMainWindow, QWidget {
    background-color: #1f1f1f;
}

/* 2. Heading Style (Neon Purple + Underline) */
QLabel {
    color: #e0b0ff; /* Neon Purple */
    font-size: 36px;
    font-weight: bold;
    font-family: &quot;Segoe UI Black&quot;, &quot;Arial&quot;;
    qproperty-alignment: AlignCenter;
    
    padding-bottom: 15px;
    border-bottom: 4px solid #8e44ad; /* Purple Line */
    margin-bottom: 30px;
}

/* 3. General Buttons Design (Exit button bhi yahi style lega) */
QPushButton {
    background-color: #000000;  /* Pitch Black */
    color: white;               /* White Text */
    font-weight: bold;
    font-size: 14px;
    border: 2px solid #9b59b6;  /* Purple Border */
    border-radius: 12px;        /* Gol Konay */
    padding: 10px;              
    
    /* Size Settings */
    min-width: 200px;
    max-width: 300px;
    min-height: 45px;
    
    margin: 5px;
}

/* 4. Hover Effect (Sab buttons par same chalega) */
QPushButton:hover {
    background-color: #8e44ad; /* Purple Glow */
    border: 2px solid white;
    color: white;
}

/* 5. Pressed Effect */
/* 5. Pressed Effect (Dabanay par) */
QPushButton:pressed {
    background-color: #5b2c6f; /* Thora aur Dark Purple */
    border: 2px solid #8e44ad; /* Border color change */
    /* transform: scale(0.98);  &lt;-- Ye line mita di kyunki Qt support nahi karta */
    padding-top: 12px; /* Juggad: Padding barha di taake button dabta hua feel ho */
    padding-bottom: 8px;
}

/* 6. Exit Button Spacing (Sirf faasla rakha hai, rang normal hai) */
QPushButton#pushButton_Exit {
    margin-top: 20px; /* Thora neechay taake alag dikhe */
}</string>
